# Configuration pour Agent MCP QA
# Copier ce fichier vers .env et remplir les valeurs

# OpenAI API Key (obligatoire pour la generation de scripts)
OPENAI_API_KEY= your_key

# Jenkins Configuration (optionnel)
JENKINS_URL=http://localhost:8080
JENKINS_USERNAME=admin
JENKINS_API_TOKEN=votre_token_jenkins

# GitLab CI Configuration (optionnel)
GITLAB_URL=https://gitlab.com
GITLAB_PRIVATE_TOKEN=votre_token_gitlab

# Jira Configuration (optionnel)
JIRA_URL=https://votre-instance.atlassian.net
JIRA_EMAIL=votre.email@example.com
JIRA_API_TOKEN=votre_token_jira
